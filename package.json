{
  "name": "app_core",
  "version": "1.0.0",
  "main": "src/App.js",
  "scripts": {
    "lint": "eslint --ext .jsx,.js src/",
    "lint-apply": "eslint --ext .jsx,.js src/ --fix",
    "unit-test": "jest",
    "build-android-dev-client": "eas build --platform android --local --profile dev-client --non-interactive --output bin/android_dev-client.apk",
    "build-android-dev": "eas build --platform android --local --profile dev --non-interactive --output bin/android_dev.apk",
    "build-android-preview": "eas build --platform android --local --profile preview --non-interactive --output bin/android_preview.apk",
    "start-expo-client": "expo start --dev-client"
  },
  "dependencies": {
    "eas-cli": "^4.1.1",
    "expo": "~49.0.5",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "react": "18.2.0",
    "react-native": "0.72.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/eslint-parser": "^7.22.9",
    "eslint": "^8.46.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-native": "^4.0.0",
    "expo-dev-client": "^2.4.6",
    "jest": "^29.6.2"
  },
  "private": true
}
